{
    "name": "sentech/media-manager",
    "description": "Wordpress flavoured",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Sentech\\MediaManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sentech\\MediaManager\\Tests\\" : "tests/"
        }
    },
    "authors": [
        {
            "name": "Nuwan",
            "email": "nuwanchamp02@gmail.com"
        }
    ],
    "require": {
        "spatie/laravel-medialibrary":"^9.0.0",
        "orchestra/testbench": "^6.24",
        "illuminate/filesystem": "^8.81"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.7"
    },
    "extra": {
        "laravel":{
            "providers": [
                "Sentech\\MediaManager\\MediaManagerServiceProvider"
            ],
            "aliases": {}
        }
    },
    "minimum-stability": "dev",
}
